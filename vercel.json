{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/wp-json/:path*",
      "destination": "https://tsf.dvj.mybluehost.me/wp-json/:path*"
    },
    {
      "source": "/wp-admin/:path*",
      "destination": "https://tsf.dvj.mybluehost.me/wp-admin/:path*"
    },
    {
      "source": "/wp-login.php",
      "destination": "https://tsf.dvj.mybluehost.me/wp-login.php"
    },
    {
      "source": "/wp-content/:path*",
      "destination": "https://tsf.dvj.mybluehost.me/wp-content/:path*"
    },
    {
      "source": "/wp-includes/:path*",
      "destination": "https://tsf.dvj.mybluehost.me/wp-includes/:path*"
    },
    {
      "source": "/wp-sitemap.xml",
      "destination": "https://tsf.dvj.mybluehost.me/wp-sitemap.xml"
    },
    {
      "source": "/wp-sitemap:path*.xml",
      "destination": "https://tsf.dvj.mybluehost.me/wp-sitemap:path*.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "https://tsf.dvj.mybluehost.me/robots.txt"
    },
    {
      "source": "/tag/:path*",
      "destination": "https://tsf.dvj.mybluehost.me/tag/:path*"
    },
    {
      "source": "/category/:path*",
      "destination": "https://tsf.dvj.mybluehost.me/category/:path*"
    }
  ]
}
